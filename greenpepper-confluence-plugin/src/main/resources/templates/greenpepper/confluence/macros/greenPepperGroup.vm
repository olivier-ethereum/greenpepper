<div id="conf_group_$bulkUID" width="100%" style="display:inline;">
	<span id="conf_exeButton_$bulkUID" class="conf_button" onclick="conf_greenPepper.runBulk('$bulkUID');"><img style="vertical-align:bottom;" src="$req.contextPath/download/resources/com.greenpepper.confluence.plugin:greenpepper.webactions/images/execute.gif" alt="" border="0"></span><span id="conf_exeButton_shadow_$bulkUID" style="display:none"><img style="vertical-align:bottom;" src="$req.contextPath/download/resources/com.greenpepper.confluence.plugin:greenpepper.webactions/images/executing.gif" alt="" border="0"></span>
	<span id="conf_executionTitle_$bulkUID" class="conf_executionTitle">
		#if($title)$title
		#else$greenPepper.getText('greenpepper.group.group')&nbsp;$bulkUID
		#end
	</span>
	<span id="conf_waiting_display_$bulkUID" style="display:; padding: 0px 15px; vertical-align:bottom;"><img src="$req.contextPath/download/resources/com.greenpepper.confluence.plugin:greenpepper.page/images/wait.gif" alt="" border="0"></span>
	<script language="JavaScript" type="text/javascript">addLoadEvent("GP.View.fade('conf_waiting_display_$bulkUID', 0.3);");</script>
</div>